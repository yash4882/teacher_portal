<h1>Log in</h1>

<%= form_with model: @teacher, url: session_path do |form| %>

	<div>
		<%= form.label :email %>
		<%= form.email_field :email %>
	</div>
	
	<div>
		<%= form.label :password %>
		<%= form.password_field :password %>
	</div>

	<div>
		<%= form.submit "Log in"%>
	</div>
<% end %>

<%= link_to "Forget Password", new_password_reset_path %>